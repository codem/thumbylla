{
  "name": "codem-thumbylla",
  "version": "2.0.0",
  "description": "Thumbor image generation for Silverstripe 4",
  "license": "MIT",
  "author": "James Ellis",
  "engines": {
    "node": "^8.x"
  },
  "scripts": {
     "build": "yarn && NODE_ENV=production webpack -p --bail --progress",
     "dev": "NODE_ENV=development webpack --progress",
     "watch": "yarn && NODE_ENV=development webpack --watch --progress",
     "css": "yarn && WEBPACK_CHILD=css webpack -p --bail --progress",
     "lock": "npm-shrinkwrap --dev",
     "lint": "eslint client/src && sass-lint -v client/src"
   },
  "devDependencies": {
    "@silverstripe/webpack-config": "^0.7"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.3.1",
    "react-cropper": "^1.0"
  }
}
